<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent"
				app:layout_constraintBottom_toBottomOf="parent"
				tools:ignore="UselessParent">
			<ScrollView
					android:layout_width="match_parent"
					android:layout_height="match_parent">
				<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:layout_marginTop="0dp"
						android:layout_marginBottom="0dp"
						android:layout_marginLeft="0dp"
						android:layout_marginRight="0dp"
						tools:ignore="ObsoleteLayoutParam">
					<androidx.viewpager.widget.ViewPager
							android:id="@+id/viewpager"
							android:layout_width="match_parent"
							android:layout_height="400dp"
							android:layout_centerHorizontal="true"
							android:layout_marginTop="0dp"
							android:layout_alignParentTop="true"/>
					<androidx.constraintlayout.widget.ConstraintLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content">
						<Button
								android:id="@+id/btnbeelin1"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:textStyle="bold"
								android:text="@string/tarif_rejalar"
								android:drawableLeft="@drawable/ic_sim_card"
								android:paddingStart="10sp"
								android:background="@drawable/shadovbeeline"
								android:layout_marginTop="16dp"
								app:layout_constraintTop_toTopOf="parent"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								style="@android:style/Widget.Button"
								app:layout_constraintHorizontal_bias="0.0"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin2"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_internet"
								android:paddingStart="10sp"
								style="@android:style/Widget.Button"
								android:text="@string/internet_to_plamlar"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin1"
								
								android:background="@drawable/shadovbeeline"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								android:layout_marginTop="20sp"
								app:layout_constraintHorizontal_bias="0.504"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin3"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_message"
								android:paddingStart="10sp"
								style="@android:style/Widget.Button"
								android:text="@string/sms_to_plamlar"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin2"
								android:background="@drawable/shadovbeeline"
								android:layout_marginTop="20sp"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintEnd_toStartOf="@+id/guideline5" tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin4"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_alarm"
								android:paddingStart="10sp"
								android:textStyle="bold"
								android:layout_marginTop="20sp"
								style="@android:style/Widget.Button"
								android:background="@drawable/shadovbeeline"
								android:text="@string/daqiqa_to_plamlar"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin3"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin5"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_menu"
								android:paddingStart="10sp"
								android:text="@string/hizmatlar"
								style="@android:style/Widget.Button"
								android:layout_marginTop="20sp"
								android:background="@drawable/shadovbeeline"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin4"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintHorizontal_bias="0.504"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin6"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_dialpad"
								android:paddingStart="10sp"
								android:text="@string/ussd_kodlar"
								style="@android:style/Widget.Button"
								android:layout_marginTop="20sp"
								android:background="@drawable/shadovbeeline"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin5"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintHorizontal_bias="0.504"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin7"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_balance"
								android:paddingStart="10sp"
								android:text="@string/balans_tekshirish"
								android:layout_marginTop="20sp"
								style="@android:style/Widget.Button"
								android:background="@drawable/shadovbeeline"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin6"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<Button
								android:id="@+id/btnbeelin8"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:textSize="25sp"
								android:textStyle="bold"
								android:textAlignment="textStart"
								android:textColor="@color/white"
								android:drawableLeft="@drawable/ic_phone"
								android:paddingStart="10sp"
								android:text="@string/raqamni_aniqlash"
								android:layout_marginTop="20sp"
								style="@android:style/Widget.Button"
								android:background="@drawable/shadovbeeline"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin7"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintHorizontal_bias="0.495"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								tools:ignore="RtlHardcoded,RtlSymmetry"/>
						<androidx.constraintlayout.widget.Guideline
								android:orientation="vertical"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:id="@+id/guideline4"
								app:layout_constraintGuide_percent="0.04"/>
						<androidx.constraintlayout.widget.Guideline
								android:orientation="vertical"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								app:layout_constraintGuide_percent="0.96"
								android:id="@+id/guideline5"/>
						<View
								android:id="@+id/view"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:background="@drawable/shadovimgback"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								app:layout_constraintHorizontal_bias="0.0"
								app:layout_constraintTop_toBottomOf="@+id/view2"/>
						<View
								android:id="@+id/view2"
								android:layout_width="0dp"
								android:layout_height="55dp"
								android:background="@drawable/shadovbeeline"
								app:layout_constraintStart_toStartOf="@+id/guideline4"
								app:layout_constraintEnd_toStartOf="@+id/guideline5"
								app:layout_constraintHorizontal_bias="1.0"
								app:layout_constraintTop_toBottomOf="@+id/btnbeelin8"
								android:layout_marginTop="20dp"/>
						<androidx.constraintlayout.widget.Guideline
								android:orientation="vertical"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								app:layout_constraintGuide_percent="0.74"
								android:id="@+id/guideline6"/>
						<View
								android:layout_width="35dp"
								android:layout_height="35dp"
								android:background="@drawable/shadovimgback"
								app:layout_constraintTop_toTopOf="@+id/imgbeelin2" android:id="@+id/view4"
								app:layout_constraintStart_toStartOf="@+id/imgbeelin2"
								app:layout_constraintEnd_toEndOf="@+id/imgbeelin2"
								app:layout_constraintBottom_toBottomOf="@+id/imgbeelin2"/>
						
						<View
								android:layout_width="35dp"
								android:layout_height="35dp"
								android:background="@drawable/shadovimgback"
								app:layout_constraintTop_toTopOf="@+id/imgbeelin2"
								app:layout_constraintStart_toStartOf="@+id/imgbeelin1"
								app:layout_constraintEnd_toEndOf="@+id/imgbeelin1"
								app:layout_constraintBottom_toBottomOf="@+id/imgbeelin1"
								android:id="@+id/view3"/>
						
						<ImageView
								android:id="@+id/imgbeelin3"
								android:layout_width="35dp"
								android:layout_height="35dp"
								app:srcCompat="@drawable/ic_instagram"
								app:layout_constraintBottom_toTopOf="@+id/view"
								app:layout_constraintTop_toTopOf="@+id/view2"
								app:layout_constraintEnd_toStartOf="@+id/guideline6"
								app:layout_constraintStart_toStartOf="@+id/guideline6"
								tools:ignore="ContentDescription"/>
						<ImageView
								android:id="@+id/imgbeelin2"
								android:layout_width="20dp"
								android:layout_height="20dp"
								app:srcCompat="@drawable/ic_web"
								android:tint="@color/orange"
								app:layout_constraintBottom_toTopOf="@+id/view"
								app:layout_constraintTop_toTopOf="@+id/view2"
								app:layout_constraintEnd_toStartOf="@+id/guideline8"
								app:layout_constraintStart_toStartOf="@+id/guideline8"
								tools:ignore="ContentDescription"/>
						<ImageView
								android:id="@+id/imgbeelin1"
								android:layout_width="20dp"
								android:layout_height="20dp"
								android:tint="@color/orange"
								app:srcCompat="@drawable/ic_operator"
								app:layout_constraintBottom_toTopOf="@+id/view"
								app:layout_constraintTop_toTopOf="@+id/view2"
								app:layout_constraintEnd_toStartOf="@+id/guideline7"
								app:layout_constraintStart_toStartOf="@+id/guideline7"
								tools:ignore="ContentDescription"/>
						<androidx.constraintlayout.widget.Guideline
								android:orientation="vertical"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								app:layout_constraintGuide_percent="0.25"
								android:id="@+id/guideline7"/>
						<androidx.constraintlayout.widget.Guideline
								android:orientation="vertical"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								app:layout_constraintGuide_percent="0.5"
								android:id="@+id/guideline8"/>
					</androidx.constraintlayout.widget.ConstraintLayout>
				
				</LinearLayout>
			</ScrollView>
			
		</RelativeLayout>
</FrameLayout>